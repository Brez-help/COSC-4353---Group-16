{
    "require-dev": {
        "phpunit/phpunit": "9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\":"app"
        }
    },
    "unit-test": [
        "@phpunit --log-junit build/log/junit.xml --coverage-clover build/log/clover.xml test/unit",
        "@php -f lib/build/coverage-checker.php -- build/log/clover.xml"
      ],
    "require": {
        "phpunit/php-code-coverage": "^9.2"
    }
}
